@model Orkidea.SchoolPanel.WebFront.Models.vmCourse

@{
    ViewBag.Title = "Delete";
}

<h2>Maestro de cursos</h2>
    @Html.HiddenFor(model => model.id)
    @Html.HiddenFor(model => model.idColegio)
    @Html.HiddenFor(model => model.idGrado)
    @Html.HiddenFor(model => model.idDirectorCurso)

    <div class="row-fluid">
            <label>Grado</label>
            @Html.TextBoxFor(model => model.desGrado, new { readOnly=true })
        </div>
        <div class="row-fluid">
            <label>Año</label>
            @Html.TextBoxFor(model => model.ano, new { readOnly=true })
        </div>
        <div class="row-fluid">
            <label>Nombre curso</label>
            @Html.TextBoxFor(model => model.Descripcion, new { readOnly=true })
        </div>
        <div class="row-fluid">
            <label>Director de curso</label>
            @Html.TextBoxFor(model => model.desDirectorCurso, new { readOnly=true })
        </div>

@using (Html.BeginForm())
{
    <div class="row-fluid">
        <div class="span3">
            <p>
                <input type="submit" value="Eliminar" class="btn btn-info" />|
                    @Html.ActionLink("Volver a la lista", "Index")
            </p>
        </div>
    </div>
}

